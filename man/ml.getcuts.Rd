## The function is currently defined as
function (data, n.quantiles) 
{
    cuts <- matrix(0, n.quantiles + 1, dim(data)[2])
    I <- dim(data)[2]
    for (i in 1:I) {
        cuts[, i] <- quantile(data[, i], probs = c(0:n.quantiles/n.quantiles))
        cuts[1, i] <- -1e+32
        cuts[n.quantiles + 1, i] <- 1e+32
    }
    return(cuts)
  }
}
